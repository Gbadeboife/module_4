# Server
PORT=3049

# Redis connection (used by server.js and seed.js)
REDIS_URL=redis://localhost:6379

# Jaeger/OpenTelemetry exporter endpoint (used by tracing.js)
JAEGER_ENDPOINT=http://localhost:14268/api/traces

# High-load integration test configuration (used by integration.highload.test.js)
# Event ID to test
TEST_EVENT_ID=1
# Total number of requests to send
TEST_REQUESTS=5000
# Requests per batch (throttling)
TEST_BATCH=500

